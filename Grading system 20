#include <stdio.h>
#include <string.h>
int main() 
{
    char studentName[100];
    int m1, m2, m3, m4, m5;
    int total;
    float average;
    char grade[10];
    scanf("%s", studentName);
    scanf("%d", &m1);
    scanf("%d", &m2);
    scanf("%d", &m3);
    scanf("%d", &m4);
    scanf("%d", &m5);
    total = m1 + m2 + m3 + m4 + m5;
    average = total / 5.0;
    if (average >= 90)
        strcpy(grade, "A");
    else if (average >= 80)
        strcpy(grade, "B");
    else if (average >= 70)
        strcpy(grade, "C");
    else if (average >= 60)
        strcpy(grade, "D");
    else if (average >= 50)
        strcpy(grade, "E");
    else
        strcpy(grade, "Fail");
    printf("Name of the Student:%s\n", studentName);
    printf("Total Mark:%d\n", total);
    printf("Average Mark:%.1f\n", average);
    printf("Grade Mark:%s\n", grade);
    return 0;
}
